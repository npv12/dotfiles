alias git="noglob git"
alias g="git"
alias ga="forgit::add"
alias gav="git add --verbose"
alias gap="git apply"
alias gbr="git branch"
alias gbrnm="git branch --no-merged"
alias gbl="forgit::blame"
alias gbs="git bisect"
alias gcm="git commit -s -S -v"
alias gcm!="git commit -s -S -v --amend"
alias gcmn="git commit -s -S -v --no-edit"
alias gcmn!="git commit -s -S -v --no-edit --amend"
alias gco="forgit::checkout::branch"
alias gcor="git checkout --recurse-submodules"
alias gcp="git cherry-pick -s -S"
alias gcpb="git cherry-pick -s -S --strategy=recursive -X ours"
alias gcpa="git cherry-pick --abort"
alias gcpc="git cherry-pick --continue"
alias gcps="git cherry-pick --skip"
alias gd="forgit::diff"
alias gf="git fetch"
alias gp="git push"
alias gpl="git pull"
alias gfo="git fetch origin"
alias gm="git merge"
alias grem="git remote"
alias grb="forgit::rebase"
alias gr="git reset"
alias grh="forgit::reset::head"
alias grh!="git reset --hard HEAD"
alias gst="gitstatus"
alias gl="forgit::log"
alias gsl="git shortlog"
alias gcount="git -sn"
alias gstats="git log --graph --pretty='%Cred%h%Creset -%C(auto)%d%Creset %s %Cgreen(%ar) %C(bold blue)by %an%Creset' --all"
alias glast="git log -1 HEAD"
alias gclone="git clone"
alias glo="git log --oneline --decorate"
alias gcmai="generate_commit"
