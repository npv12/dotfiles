{
  "agent": {
    "default_model": {
      "provider": "copilot_chat",
      "model": "oswe-vscode-prime"
    },
    "favorite_models": [],
    "model_parameters": []
  },
  "git_panel": {
    "tree_view": true
  },
  "base_keymap": "JetBrains",
  "icon_theme": "Catppuccin Mocha",
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "lsp": {
    "jdtls": {
      "settings": {
        "java": {
          "home": "$HOME/.local/share/mise/installs/java/21.0.2/",
          "jdt": {
            "ls": {
              "lombokSupport": {
                "enabled": true
              }
            }
          }
        }
      }
    }
  },
  "autosave": {
    "after_delay": {
      "milliseconds": 500
    }
  },
  "terminal": {
    "font_family": "MesloLGS NF",
    "font_size": 14,
    "line_height": "standard"
  },
  "when_closing_with_no_tabs": "keep_window_open",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "theme": {
    "mode": "dark",
    "light": "One Light",
    "dark": "Catppuccin Mocha (Blur)"
  }
}
