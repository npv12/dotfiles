{
  "icon_theme": "Catppuccin Mocha",
  "features": {
    "edit_prediction_provider": "copilot"
  },
  "agent": {
    "default_profile": "write",
    "default_model": {
      "provider": "copilot_chat",
      "model": "claude-sonnet-4"
    }
  },
  "when_closing_with_no_tabs": "keep_window_open",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Catppuccin Mocha (Blur)"
  }
}
